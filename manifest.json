  "description": "Applies modern custom CSS styles to progtest.fit.cvut.cz.",
  "icons": {
    "128": "./Icon.png"
  },
  "content_scripts": [
    {
      "matches": ["*://progtest.fit.cvut.cz/*"],
      "css": ["css.css"]
    }
  ]
}